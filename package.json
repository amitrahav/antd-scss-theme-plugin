		"less": "^3.11.1",
		"less-loader": "^6.0.0",
		"node-sass": "^4.14.0",
		"prettier": "^2.0.5",
		"raw-loader": "^4.0.1",
		"rimraf": "^3.0.2",
		"sass-loader": "^8.0.2",
		"webpack": "^4.43.0",
		"webpack-node-externals": "^1.7.2"
	}
