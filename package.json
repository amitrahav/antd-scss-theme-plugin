		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.9.5",
		"antd": "^4.1.5",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-add-module-exports": "^1.0.2",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-2": "^6.24.1",
		"cross-env": "^7.0.2",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
